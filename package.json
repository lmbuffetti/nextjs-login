{
  "name": "recipe-book",
  "author": "Luigi Mattia Buffetti",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dependencies:fix": "syncpack fix-mismatches",
    "dependencies:lint": "syncpack lint-mismatches",
    "dependencies:update": "syncpack update",
    "pre-commit": "next lint && tsc",
    "dev": "next dev",
    "dev-turbo": "turbo run dev",
    "build": "turbo builds",
    "builds": "pnpm test-dom && next lint --fix && pnpm test-node && next build",
    "start": "next start",
    "lint": "next lint --fix && tsc",
    "lint-turbo": "turbo run lint",
    "lint:fix": "next lint --fix",
    "lint-turbo:fix": "turbo run lint:fix",
    "postinstall": "pnpm dependencies:fix && pnpm dependencies:update",
    "test-dom": "NODE_ENV=test jest ./src/__tests__ --config ./jest.config.dom.ts",
    "test-node": "NODE_ENV=test jest ./src/__tests__ --config ./jest.config.node.ts --detectOpenHandles --forceExit",
    "cypress:open": "cypress open",
    "e2e": "cypress run --e2e --headless --browser chrome",
    "test-cypress": "next start & npm run e2e",
    "create-doc": "swagger-cli bundle src/api/Swagger/index.yaml --outfile src/api/Swagger/index.json",
    "vercel-build": "pnpm global add node-gyp sharp && pnpm build"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@next-auth/mongodb-adapter": "1.1.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@typegoose/typegoose": "12.9.0",
    "@types/bcrypt": "5.0.2",
    "@types/jest": "29.5.14",
    "@types/mocha": "10.0.9",
    "@types/ms": "0.7.34",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/swagger-ui-react": "4.18.3",
    "@vercel/blob": "0.26.0",
    "autoprefixer": "10.4.20",
    "bcrypt": "5.1.1",
    "dotenv": "16.4.5",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-formatjs": "5.2.5",
    "eslint-plugin-jsdoc": "50.5.0",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "formatjs": "0.1.1",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb": "6.10.0",
    "mongoose": "8.8.2",
    "ms": "2.1.3",
    "next": "15.0.3",
    "next-auth": "4.24.10",
    "next-static-manifest": "0.9.0",
    "nextjs-toploader": "^3.7.15",
    "node-mocks-http": "1.16.1",
    "postcss": "8.4.49",
    "pre-commit": "1.2.2",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.5",
    "react-hot-toast": "2.4.1",
    "react-image-crop": "11.0.7",
    "react-select": "5.8.3",
    "sharp": "0.33.5",
    "swagger-cli": "4.0.4",
    "swagger-ui-react": "5.18.2",
    "swr": "2.2.5",
    "syncpack": "13.0.0",
    "tailwindcss": "3.4.15",
    "ts-node": "latest",
    "turbo": "2.3.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.13.7",
    "@types/node": "22.9.1",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "cypress": "13.16.0",
    "cypress-image-diff-js": "2.3.0",
    "eslint": "9.15.0",
    "eslint-plugin-tailwindcss": "3.17.5",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "packageManager": "pnpm@8.3.0"
}
