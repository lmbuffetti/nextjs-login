{
  "name": "recipe-book",
  "author": "Luigi Mattia Buffetti",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dependencies:fix": "syncpack fix-mismatches",
    "dependencies:lint": "syncpack lint-mismatches",
    "dependencies:lock": "syncpack set-semver-ranges",
    "pre-commit": "next lint && tsc",
    "dev": "next dev",
    "dev-turbo": "turbo run dev",
    "build": "turbo builds",
    "builds": "pnpm test-dom && pnpm test-node && next build",
    "start": "next start",
    "lint": "next lint --fix && tsc",
    "lint-turbo": "turbo run lint",
    "lint:fix": "next lint --fix",
    "lint-turbo:fix": "turbo run lint:fix",
    "postinstall": "pnpm dependencies:fix && pnpm dependencies:lock",
    "test-dom": "NODE_ENV=test jest ./src/__tests__ --config ./jest.config.dom.ts",
    "test-node": "NODE_ENV=test jest ./src/__tests__ --config ./jest.config.node.ts --detectOpenHandles --forceExit",
    "cypress:open": "cypress open",
    "e2e": "cypress run --e2e --headless --browser chrome",
    "test-cypress": "next start & npm run e2e",
    "create-doc": "swagger-cli bundle src/api/Swagger/index.yaml --outfile src/api/Swagger/index.json"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@next-auth/mongodb-adapter": "1.1.3",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@typegoose/typegoose": "12.0.0",
    "@types/bcrypt": "5.0.2",
    "@types/jest": "29.5.10",
    "@types/mocha": "10.0.6",
    "@types/ms": "0.7.34",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@types/swagger-ui-react": "4.18.3",
    "@vercel/blob": "0.15.1",
    "autoprefixer": "10.4.16",
    "bcrypt": "5.1.1",
    "dotenv": "16.3.1",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-formatjs": "4.11.3",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-no-relative-import-paths": "1.5.3",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "formatjs": "0.1.1",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "mongodb": "6.3.0",
    "mongoose": "8.0.2",
    "ms": "2.1.3",
    "next": "14.0.3",
    "next-auth": "4.24.5",
    "next-static-manifest": "0.9.0",
    "node-mocks-http": "1.13.0",
    "postcss": "8.4.32",
    "pre-commit": "1.2.2",
    "prettier": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-hot-toast": "2.4.1",
    "react-image-crop": "11.0.1",
    "react-select": "5.8.0",
    "sharp": "0.33.0",
    "swagger-cli": "4.0.4",
    "swagger-ui-react": "5.10.3",
    "swr": "2.2.4",
    "syncpack": "11.2.1",
    "tailwindcss": "3.3.5",
    "ts-node": "latest",
    "turbo": "1.10.16"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.12.12",
    "@types/node": "20.10.2",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "cypress": "13.6.0",
    "cypress-image-diff-js": "1.32.0",
    "eslint": "8.55.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
